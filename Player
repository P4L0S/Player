int x=160;
int y=160;

void ejecutar(){
  player();
  controles();
}

void main(){
  animate(ejecutar);
}
  
  
  
void player(){
  image("https://i.blogs.es/cc8912/screenshot_1764/450_1000.webp",0,0,400,400);
  image("https://seeklogo.com/images/Z/Zelda-logo-687714F8A8-seeklogo.com.png",x,y,150,150);
}

void controles(){
  if(keyPressed && key=="up"){
    println("Opa");
    y = y -1;}
    if(keyPressed && key=="down"){
      print("Ayiyi");
      y = y +1;
    }
  }
